package Package;

public class playWithString {
	public static void main(String[] args) {
		String name = "This is Ramya";
		String arr[] = name.split(" ");
		
		for (int i = 0; i < arr.length; i++) {
			for (int j = arr[i].length();j > 0; j--) {
				System.out.println(arr[i].charAt(j));
			}
			
		}
		
	}

}
