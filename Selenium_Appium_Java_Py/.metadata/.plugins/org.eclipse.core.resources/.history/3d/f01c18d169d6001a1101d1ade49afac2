package Package;

public class playWithString {
	public static void main(String[] args) {
		
		//type 1
		String name = "This is Ramya";
		String arr[] = name.split(" ");
		
		for (int i = 0; i < arr.length; i++) {
			for (int j = arr[i].length()-1;j >= 0; j--) {
				System.out.print(arr[i].charAt(j));
			}
			System.out.print(" ");
		}
		
		//type 2
		
	}

}
